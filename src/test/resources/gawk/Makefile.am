NEED_LOCALE_JP = mbprintf2

NEED_LOCALE_RU = mtchi18n

# List of tests that fail on MinGW
EXPECTED_FAIL_MINGW = \
	backbigs1 backsmalls1 clos1way6 close_status dbugeval4\
	devfd devfd1 devfd2 errno exitval2 fork fork2 fts functab5 \
